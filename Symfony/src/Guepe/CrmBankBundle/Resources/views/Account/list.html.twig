{% for cat in accounts %}
	<div class="list-big-element">&nbsp;<img valign="middle"  src="{{ asset('bundles/guepecrmbank/images/icons/account-list.png') }}" > {{ cat.name }}    <a href="{{ path('ShowAccount',{'id': cat.id}) }}"><img src="{{ asset('bundles/guepecrmbank/images/icons/show.png') }}" ></a>  <a href="{{ path('EditAccount',{'id': cat.id}) }}"><img src="{{ asset('bundles/guepecrmbank/images/icons/edit.png') }}" ></a>
	</div>
	<br/>
	<strong>Statut:</strong> {{ cat.companystatut }}
	<strong>Type: </strong>{{ cat.type }}
	<strong>Début de la relation: </strong>{{ cat.startingdate|date('d/m/Y') }}
	<p>
	<strong>Notes: </strong>{{ cat.notes }}
	</p>
{% else %}
	{% block warning %}
	<div class="warning">Aucun account n'a été trouvé.</div>
	{% endblock %}
{% endfor %}
	
	
	