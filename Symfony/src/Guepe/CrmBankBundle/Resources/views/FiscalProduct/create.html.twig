{% extends 'GuepeCrmBankBundle::layout.html.twig' %}

{% block title %}Ajout de produit Fiscal{% endblock %}

{% form_theme form 'GuepeCrmBankBundle:Form:fields.html.twig' %}

{% block content %}

<script type="text/javascript">
	$(function(){
		// Datepicker
		$('#guepe_crmbankbundle_fiscalproducttype_startdate').datepicker($.datepicker.regional["fr"]);
		$('#guepe_crmbankbundle_fiscalproducttype_enddate').datepicker($.datepicker.regional["fr"]);
		$('#guepe_crmbankbundle_fiscalproducttype_paymentdate').datepicker($.datepicker.regional["fr"]);
		$('#guepe_crmbankbundle_fiscalproducttype_reservedate').datepicker($.datepicker.regional["fr"]);

		
	});
</script>
<form action="" method="post" {{ form_enctype(form) }}>
    <table>
    {{ form_widget(form) }}
    </table>
    <p>
        <button type="submit">Enregistrer</button>
    </p>
</form>
{% endblock %}