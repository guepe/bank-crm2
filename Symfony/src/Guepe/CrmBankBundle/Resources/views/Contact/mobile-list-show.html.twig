{% for contact in contacts %}
	<div data-role="page" id="Account_{{ contact.id }}">
		<div data-role="header" >
			<a href="#home" id="back">Retour</a>
			<h1>{{ contact.lastname }} {{ contact.firstname }}</h1>
		</div><!-- /header -->
		<div data-role="content">		
		<br/><img valign="middle" src="{{ asset('bundles/guepecrmbank/images/icons/mail.png') }}" > <a href="mailto:{{contact.email}}">{{contact.email}}</a>
		<br/> {{contact.streetnum}}<br/> {{contact.zip}} {{contact.city}} {{contact.country}}
		<br/><strong>Tél:</strong> {{contact.phone}}
		<br/><strong>Gsm:</strong> {{contact.gsm}}
		<br/><strong>Date et lieu de naissance:</strong> {{contact.birthplace}} {{contact.birthdate|date('d/m/Y')}}
		<br/><strong>Numéro de carte d'identité:</strong> {{contact.eid}}
		<br/><strong>Numéro nationnal:</strong> {{contact.niss}}
		<br/><strong>Statut matital:</strong> {{contact.maritalstatus}}
		<div>
			<br/><strong><u>Rentrées:</u></strong>
			<ul>
			<strong>Montant:</strong> {{contact.incomeamount}}
			<br/><strong>Récurrence:</strong> {{contact.incomerecurence}}
			<br/><strong>Date des rentrées:</strong> {{contact.incomedate|date('d/m/Y')}}
			</ul>
		</div>
		</div>
		</div><!-- /content -->
	</div><!-- /page {{ contact.id }} -->	
{% endfor %}

	